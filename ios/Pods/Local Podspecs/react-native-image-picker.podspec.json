{
  "name": "react-native-image-picker",
  "version": "5.7.0",
  "summary": "A React Native module that allows you to use native UI to select media from the device library or directly from the camera",
  "license": "MIT",
  "authors": "Johan du Toit (Johan-dutoit)",
  "homepage": "https://github.com/react-native-image-picker/react-native-image-picker",
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/react-native-image-picker/react-native-image-picker.git",
    "tag": "v5.7.0"
  },
  "source_files": "ios/*.{h,m,mm}",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/boost-for-react-native\" \"$(PODS_ROOT)/RCT-Folly\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17"
  },
  "compiler_flags": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -Wno-comma -Wno-shorten-64-to-32 -DRN_FABRIC_ENABLED -fmodules -fcxx-modules",
  "dependencies": {
    "React": [

    ],
    "React-RCTFabric": [

    ],
    "React-Codegen": [

    ],
    "RCT-Folly": [

    ],
    "RCTRequired": [

    ],
    "RCTTypeSafety": [

    ],
    "ReactCommon/turbomodule/core": [

    ]
  }
}
